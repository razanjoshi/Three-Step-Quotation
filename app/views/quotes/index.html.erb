<p id="notice"><%= notice %></p>

<h1>Quotes</h1>

<table class="table">
  <thead>
    <tr>
      <th>User email</th>
      <th> Quote Id</th>
      <th>Cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.user_email %></td>
        <td><%= quote.q_key %></td>
        <td><%= quote.cost %></td>
        <td><%= link_to 'Show', quote %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path, class: 'btn btn-primary' %>
